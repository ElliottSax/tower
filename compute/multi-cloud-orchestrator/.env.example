# Multi-Cloud Orchestrator Environment Variables
# Copy this file to .env and fill in your actual API keys

# Cloudflare Workers
CLOUDFLARE_API_URL=https://your-worker.your-subdomain.workers.dev
CLOUDFLARE_API_KEY=your_cloudflare_api_key_here

# AWS Lambda
AWS_REGION=us-east-1

# Google Cloud
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1

# Hugging Face
HUGGINGFACE_API_KEY=hf_your_huggingface_key_here

# DeepSeek
DEEPSEEK_API_KEY=sk-your_deepseek_key_here

# Grok / XAI
GROK_API_KEY=xai-your_grok_key_here

# OpenRouter
OPENROUTER_API_KEY=sk-or-v1-your_openrouter_key_here

# Together AI
TOGETHER_API_KEY=your_together_key_here

# Groq
GROQ_API_KEY=gsk_your_groq_key_here

# Replicate
REPLICATE_API_KEY=r8_your_replicate_key_here

# Cerebras
CEREBRAS_API_KEY=csk-your_cerebras_key_here

# Fireworks AI
FIREWORKS_API_KEY=fw_your_fireworks_key_here
