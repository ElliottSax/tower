{
  "name": "ai-sales-platform",
  "version": "1.0.0",
  "description": "AI-powered sales automation platform with multi-agent orchestration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && cp -r src/dashboard dist/dashboard",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "worker:enrichment": "tsx src/workers/enrichment.worker.ts",
    "worker:email": "tsx src/workers/email.worker.ts",
    "worker:ai": "tsx src/workers/ai.worker.ts",
    "autonomous:demo": "tsx src/autonomous/example.ts",
    "autonomous:launch": "tsx src/autonomous/index.ts",
    "db:migrate": "drizzle-kit push:pg",
    "db:generate": "drizzle-kit generate:pg",
    "db:studio": "drizzle-kit studio",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "ai",
    "sales",
    "automation",
    "langchain",
    "langgraph",
    "crm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/anthropic": "^0.3.0",
    "@langchain/community": "^0.3.0",
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "@langchain/langgraph-checkpoint-postgres": "^0.1.0",
    "@langchain/openai": "^0.3.0",
    "@qdrant/js-client-rest": "^1.9.0",
    "@react-email/components": "^0.0.15",
    "@sentry/node": "^10.38.0",
    "@sentry/profiling-node": "^10.38.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.35.0",
    "express": "^5.0.0",
    "express-openapi-validator": "^5.5.0",
    "express-rate-limit": "^7.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.0",
    "nodemailer": "^6.9.0",
    "pg": "^8.11.3",
    "stripe": "^14.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "drizzle-kit": "^0.26.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.6.0",
    "vitest": "^1.6.1"
  }
}
